{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sp500_stocks",
						"type": "DatasetReference"
					},
					"name": "sp500stocks"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SilverDest",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Date as string,",
				"          Symbol as string,",
				"          {Adj Close} as string,",
				"          Close as string,",
				"          High as string,",
				"          Low as string,",
				"          Open as string,",
				"          Volume as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sp500stocks",
				"sp500stocks derive(FolderName = Date) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     rowUrlColumn:'FolderName',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}